{
    "name": "base-react",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "lodash.template": "^4.5.0",
        "npm-check": "^5.9.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-scripts": "^5.0.1"
    },
    "scripts": {
        "start": "react-app-rewired start",
        "build": "react-app-rewired build",
        "test": "react-app-rewired test --env=jsdom",
        "eject": "react-app-rewired eject",
        "flow": "flow"
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:flowtype/recommended"
        ],
        "parserOptions": {
            "ecmaVersion": 2017,
            "sourceType": "module"
        },
        "env": {
            "browser": true,
            "node": true
        },
        "parser": "babel-eslint",
        "plugins": [
            "flowtype"
        ],
        "rules": {}
    },
    "babel": {
        "presets": [
            "env",
            "flow"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "babel-preset-flow": "^6.23.0",
        "customize-cra": "^0.4.1",
        "eslint-config-recommended": "^4.0.0",
        "eslint-plugin-flowtype": "^3.13.0",
        "eslint-plugin-react": "^7.14.3",
        "flow-bin": "^0.104.0",
        "react-app-rewire-eslint": "^0.2.3",
        "react-app-rewired": "^2.1.3"
    }
}
